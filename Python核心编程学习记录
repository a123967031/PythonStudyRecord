#method for printing dict char
dict2 = {'name': 'venus', 'port': 6969}
print 'host %(name)s is running on port %(port)d' %dict2
#Python核心编程 P167

#dict中的sorted()方法使用
dict2 = {'name': 'venus', 'port': 6969}
for eachKey in sorted(dict2):
    print 'dict2 key', eachKey, 'has value', dict2[eachKey]
